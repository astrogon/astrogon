---
import TaxaLayout from "@components/drinks/TaxaLayout.astro";
import { getTaxa, getTaxaMultiset } from "@lib/taxonomyParser";

const taxa = await getTaxa("drinks", "spirits");
const allTaxa = await getTaxaMultiset("drinks", "spirits");
---

<TaxaLayout taxonomy={"spirits"} taxa={taxa} allTaxa={allTaxa} />
